{
  "functions": {
    "app/api/convert/image/route.ts": {
      "maxDuration": 60,
      "memory": 3008
    },
    "app/api/convert/route.ts": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "headers": [
    {
      "source": "/api/convert/(.*)",
      "headers": [
        {
          "key": "max-body-size",
          "value": "10mb"
        }
      ]
    }
  ]
}
